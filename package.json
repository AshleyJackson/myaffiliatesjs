{
  "name": "myaffiliatesjs",
  "version": "1.1.0",
  "scripts": {
    "tests": "nodemon tests/tests",
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts"
  },
  "author": "AshleyJackson",
  "repository": {
    "type": "https",
    "url": "https://github.com/AshleyJackson/myaffiliatesjs"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "tsup": {
    "dts": true,
    "minify": false,
    "format": [
      "cjs",
      "esm"
    ],
    "clean": true
  },
  "dependencies": {
    "@types/node": "^20.14.2",
    "axios": "^1.7.2",
    "nodemon": "^3.1.3",
    "simple-xml-to-json": "^1.2.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  }
}
