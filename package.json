{
  "name": "myaffiliatesjs",
  "version": "2.0.4",
  "type": "module",
  "scripts": {
    "tsc": "tsc -p tsconfig.json",
    "tests": "tsx test.ts",
    "tests:decode_token": "tsx tests/decode_token.ts",
    "serve": "bun .\\tests\\serve.ts",
    "preview": "bun tsc && bun tests"
  },
  "author": "AshleyJackson",
  "repository": {
    "type": "https",
    "url": "https://github.com/AshleyJackson/myaffiliatesjs"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "tsup": {
    "dts": true,
    "minify": false,
    "format": [
      "cjs"
    ],
    "clean": true
  },
  "dependencies": {
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.14.2",
    "axios": "^1.7.2",
    "bun": "^1.1.29",
    "dotenv": "^16.4.5",
    "luxon": "^3.5.0",
    "simple-xml-to-json": "^1.2.2",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "fastify": "^5.0.0",
    "nodemon": "^3.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1"
  }
}
